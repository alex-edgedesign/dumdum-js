  "bugs": {
    "url": "https://github.com/tormjens/dumdum-js/issues"
  },
  "homepage": "https://github.com/tormjens/dumdum-js",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.23.0",
    "babel-preset-latest": "^6.16.0",
    "chai": "^3.2.0",
    "istanbul": "^0.3.5",
    "mocha": "^2.1.0",
    "pre-commit": "^1.1.1",
    "standard": "^5.0.0"
